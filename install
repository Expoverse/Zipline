version='0.0.1'
platform='linux'
os_uname=`uname`
if [[ "$os_uname" == 'Linux' ]]; then
   platform='linux'
elif [[ "$os_uname" == 'Darwin' ]]; then
   platform='darwin'
fi
curl -Lo go-backup.tar.gz https://github.com/Expoverse/go-backup/releases/download/$version/go-backup-$platform-amd64.tar.gz
tar zxf go-backup.tar.gz && sudo mv go-backup /usr/local/bin/go-backup && rm go-backup.tar.gz
mkdir -p ~/.go-backup